@model IEnumerable<interpolapi.Models.ArtificialIntelligence>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AiName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AiRole)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AiDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateCreated)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Photo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AiName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AiRole)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AiDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateCreated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Photo.PhotoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.UserId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AiId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AiId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AiId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
